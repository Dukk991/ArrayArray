#include <stdio.h>
int main(){
    int numberArray[3][3] = {
        {1,2,3},
        {4,5,6},
        {7,8,9}
    };
   
    printf("\nTaulukon ensimm√§isen rivin 1. luku on %d\n", numberArray[0][0]);
    printf("\nTaulukon toisen rivin 2. luku on %d\n", numberArray[1][1]);
    printf("\nTaulukon kolmannen rivin 3. luku on %d\n", numberArray[2][2]);
    
    return 0;
    
}
